{
  "name": "cavcpdfgen",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "docker build -t cavcpdfgen --build-arg \"CONFIG_ARG=$(<config.json)\" .",
    "deploy": "docker tag cavcpdfgen registry.heroku.com/cavcpdfgen/web && docker push registry.heroku.com/cavcpdfgen/web && heroku container:login && heroku container:release web && docker system prune -f",
    "runbuild": "docker run -p 80:3000 cavcpdfgen",
    "watch": "npx webpack --mode development --watch",
    "test": "npx mocha --require @babel/register",
    "dev-server": "npx nodemon ./bin/www config.json",
    "dev-client": "npx webpack-dev-server",
    "dev": "npx concurrently --kill-others \"npm:watch\" \"npm:dev-server\"",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.16.1",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jquery": "^3.4.1",
    "jsdom": "^15.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "pdfkit": "^0.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^1.19.0",
    "react-addons-test-utils": "^15.6.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
}
